- team['players'].each do |player|
  - statistic = player['statistics'].first["week_#{game['round'].to_s}"]
  - if statistic["game"]
    %tr{:class => player['position']}
      %td.image= player_image player, "height_22"
      %td.number= player['number']
      %td.player= link_to player['name'], player_path(player['id'])
      - Statistic.fields.each do |field|
        - unless field == 'game'
          %td{class: field}= statistic[field]
